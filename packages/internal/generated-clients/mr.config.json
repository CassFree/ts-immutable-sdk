{
  "supportsES6": true,
  "npmVersion": "6.9.0",
  "typescriptThreePlus": true,
  "withSeparateModelsAndApi": true,
  "modelPackage": "models",
  "apiPackage": "domain",
  "useSingleRequestParameter": true,
  "templateDir": "app/src/templates",
  "files": {
    "api-types.mustache": {
      "templateType": "API",
      "destinationFilename": "-types.ts"
    },
    "combined.mustache": {
      "templateType": "SupportingFiles",
      "folder": "types",
      "destinationFilename": "index.ts"
    }
  }
}
