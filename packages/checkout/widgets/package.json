{
  "name": "@imbtl/checkout-widgets",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "@biom3/design-tokens": "0.1.4-beta",
    "@biom3/react": "^0.1.3-beta",
    "@ethersproject/providers": "^5.7.2",
    "ethers": "^5.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0"
  },
  "scripts": {
    "start": "cp src/app.tsx src/index.tsx && PORT=3001 GENERATE_SOURCEMAP=false react-scripts start",
    "build": "cp src/build.tsx src/index.tsx && GENERATE_SOURCEMAP=false react-scripts build",
    "build:dist": "npm run reload:deps && npm run build && npm run dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "reload:deps": "rm -rf package-lock.json && npm run reload:sdk && npm run reload:types && npm i",
    "reload:sdk": "npm uninstall @imtbl/checkout-sdk-web --no-save",
    "reload:types": "npm uninstall @imtbl/checkout-ui-types --no-save",
    "dist": "mkdir -p ../sample-app/public/lib/js && cp build/static/js/main.*.js ../sample-app/public/lib/js/imtbl-checkout.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@imtbl/checkout-sdk-web": "0.0.0",
    "@imtbl/checkout-ui-types": "0.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "cypress": "^12.8.1",
    "local-cypress": "^1.2.6",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  }
}
