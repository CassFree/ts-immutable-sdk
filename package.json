{
  "name": "@imtbl/sdk",
  "version": "0.0.0",
  "description": "Immutable Multi-Rollup TypeScript SDK",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/base.js",
    "./starkex": "./dist/starkex.js",
    "./provider": "./dist/provider.js",
    "./passport": "./dist/passport.js",
    "./checkout": "./dist/checkout.js"
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/base.d.ts"
      ],
      "starkex": [
        "./dist/starkex.d.ts"
      ],
      "provider": [
        "./dist/provider.d.ts"
      ],
      "passport": [
        "./dist/passport.d.ts"
      ],
      "checkout": [
        "./dist/checkout.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0 <17.0.0"
  },
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production rollup --config rollup.config.js && rm -rf dist/types",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts --max-warnings=0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immutable/ts-immutable-sdk.git"
  },
  "author": "Immutable",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/immutable/ts-immutable-sdk/issues"
  },
  "homepage": "https://github.com/immutable/ts-immutable-sdk#readme",
  "dependencies": {
    "@imtbl/core-sdk": "^1.0.1",
    "@magic-ext/oidc": "^1.0.1",
    "@metamask/detect-provider": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "axios": "^1.3.4",
    "eslint": "^8.34.0",
    "ethers": "^5.7.2",
    "magic-sdk": "^13.3.1",
    "oidc-client-ts": "^2.2.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.0.0",
    "@swc/core": "^1.3.36",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "eslint": "^8.34.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "prettier": "2.8.4",
    "rollup": "^3.17.2",
    "rollup-plugin-dts": "^5.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "@types/axios": "^0.14.0"
  }
}